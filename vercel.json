{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install && npx prisma generate",
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
